{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "nixpacks",
      "buildCommand": "nest build"
    },
    "deploy": {
      "startCommand": "node dist/main",
      "healthcheckPath": "/",
      "healthcheckTimeout": 100,
      "restartPolicyType": "never"
    }
  }